  "private": true,
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.(ts|tsx|js|jsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-maps|expo|@expo|@react-navigation)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/types/**",
      "!src/**/*.stories.{ts,tsx}"
    ],
